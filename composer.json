{
    "name": "seofood/omnipay-heidelpay",
    "description": "Heidelpay driver for the Omnipay payment processing library",
    "keywords": [
        "omnipay",
        "heidelpay"
    ],
    "version": "3.0.1",
    "homepage": "https://github.com/seofood/omnipay-heidelpay",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Hillger",
            "email": "marco@seofood.de",
            "homepage": "https://seofood.de"
        }
    ],
    "require": {
        "omnipay/common": "~3.0"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Heidelpay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Heidelpay\\Tests\\": "tests/"
        }

    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
